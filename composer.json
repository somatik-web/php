{
    "name": "somatik/php",
    "description": "Somatik PHP site version",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:somatik-web/laravel.git",
            "no-api": true
        }
    ],
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^4.1",
        "monolog/monolog": "^2.0",
        "laravel/framework": "^7.0",
        "somatik/admin": "dev-master",
        "league/commonmark": "^1.4"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Evgenii Shchepotev",
            "email": "zeckson@ya.ru"
        }
    ],
    "require-dev": {
        "deployer/deployer": "^6.8"
    }
}
